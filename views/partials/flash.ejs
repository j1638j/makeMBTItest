<% if(success && success.length) { %> 
    <div class="container">
        <div class="row m-2">
            <div class="alert alert-dark alert-dismissible fade show col-md-8 offset-md-2 col-xl-4 offset-xl-4" role="alert">
                <strong><%= success %> </strong>
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            </div>
        
        </div>
    </div>
<% } %> 